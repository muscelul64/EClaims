{
  "name": "deactech-eclaims",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "start:android": "expo start --android",
    "start:ios": "expo start --ios",
    "start:web": "expo start --web",
    "start:production": "expo start --no-dev --minify",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "android:dev": "expo run:android --device",
    "android:clean": "cd android && gradlew.bat clean && cd ..",
    "android:production": "expo run:android --variant release",
    "ios": "expo run:ios",
    "ios:production": "expo run:ios --configuration Release",
    "web": "expo start --web",
    "build:android": "npx eas build --platform android",
    "build:android:preview": "npx eas build --platform android --profile preview",
    "build:android:production": "npx eas build --platform android --profile production",
    "build:ios": "npx eas build --platform ios",
    "build:ios:production": "npx eas build --platform ios --profile production",
    "deploy:production": "npx eas build --platform all --profile production",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "lint": "expo lint"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
