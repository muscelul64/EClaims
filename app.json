{
  "expo": {
    "name": "Porsche E-Claims",
    "slug": "porsche-eclaims",
    "version": "1.0.30",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "porscheeclaims",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "intentFilters": [
      {
        "action": "VIEW",
        "data": [
          {
            "scheme": "deactecheclaims"
          },
          {
            "scheme": "https",
            "host": "eclaims.deactech.com"
          }
        ],
        "category": [
          "BROWSABLE",
          "DEFAULT"
        ]
      }
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.deactech.porscheeclaims",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "buildNumber": "30",
      "associatedDomains": [
        "applinks:eclaims.deactech.com"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE"
      ],
      "package": "com.deactech.porscheeclaims",
      "versionCode": 31,
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "minSdkVersion": 23,
      "allowBackup": false,
      "networkSecurityConfig": "network_security_config",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "porscheeclaims"
            },
            {
              "scheme": "https",
              "host": "eclaims.deactech.com"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-camera",
      "expo-location",
      "expo-notifications",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-localization",
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a2392514-539f-4893-9110-b753224c3ba5"
      }
    }
  }
}
